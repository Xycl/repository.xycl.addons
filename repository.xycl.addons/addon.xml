<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="repository.xycl.addons" name="Xycl Add-on Repository" version="1.2.1" provider-name="xycl">
	<requires>
		<import addon="xbmc.addon" version="12.0.0"/>
	</requires>
	<extension point="xbmc.addon.repository" name="Xycl Add-on Repository">
		<info compressed="false">https://raw.github.com/Xycl/repository.xycl.addons/master/addons.xml</info>
